class module.exports extends Layer
	# https://fortawesome.github.io/Font-Awesome/cheatsheet/
	constructor: (options={}) ->
		options.backgroundColor ?= ''
		options.color ?= 'black'
		options.clip ?= false
		options.fontSize ?= 40
		faImported = document.getElementsByClassName('fa')
		if faImported.length is 0
			Utils.insertCSS fontAwesomeCSS
		super
		@style = fontFamily: 'FontAwesome'
	@define "icon", 
		get: -> @html
		set: (val) -> 
			val = val.replace('fa-','')
			if classNames[val]?
				@html = classNames[val]
			else
				@html = val 
	@define "fontSize",
		set: (val) -> 
			@style.fontSize = val+'px'
			@style.lineHeight = val+'px'
			style = 
				fontFamily: 'FontAwesome'
				fontSize: val+'px'
				lineHeight: val+'px'
			size = Utils.textSize @icon, style
			@width = size.width
			@height = size.height
	@define "color",
		set: (val) -> @style.color = val